<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.wz.entity">
	
	<class name="BookFormatEntity" table="wz_book_format">
		<id name="format_id" type="java.lang.Integer" column="format_id">
			<generator class="native" />
		</id>
		<property name="book_id" type="java.lang.Integer" column="book_id" />
		<property name="flag" type="java.lang.Integer" column="flag" />
	</class>
</hibernate-mapping>