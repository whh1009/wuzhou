<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.wz.entity">
	
	<class name="LogEntity" table="wz_log">
		<id name="modify_id" type="java.lang.Integer" column="modify_id">
			<generator class="native" />
		</id>
		<property name="book_serial_number" type="java.lang.String" column="book_serial_number" />
		<property name="user_name" type="java.lang.String" column="user_name" />
		<property name="modify_time" type="java.util.Date" column="modify_time" />
		<property name="modify_type" type="java.lang.String" column="modify_type" />
	</class>
</hibernate-mapping>